{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "routes": [
    {
      "src": "/data/(.*)",
      "headers": {
        "cache-control": "s-maxage=3600, stale-while-revalidate=86400"
      }
    },
    {
      "src": "/geodata/(.*)",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    }
  ],
  "functions": {
    "app/page.tsx": {
      "maxDuration": 10
    }
  }
}
